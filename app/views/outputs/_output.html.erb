<div class="output">
  <div class="upper-output">
    <div class="output-user">
      <!-- 投稿したユーザー名情報を出力する -->
      <%= output.user.name %>
    </div>
    <div class="output-date">
      <!-- 投稿した時刻を出力する -->
      <%= l output.created_at %>
    </div>
  </div>
  <div class="lower-output">
    <div class="output-content">
      <!-- 投稿したメッセージ内容を記述する -->
      <%= output.content %>
    </div>
  </div>
</div>